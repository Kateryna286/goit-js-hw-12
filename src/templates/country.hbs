
<div class="card">
    <h2 class="card-title">
        <div class="card-flag">
            <img src="{{flag}}" alt="flag {{name}}" width="30px">
        </div>
        <p class="country__title">{{name}}</p>
    </h2>

    <p class="capital">Capital: {{capital}}</p>
    <p class="country__">Population: {{population}}</p>
    <p class="lang">Languages: {{#each languages}}{{name}}, {{/each}}</p>    
    
</div>


